{
  "name": "nodejs-ts-starter",
  "version": "1.0.0",
  "description": "Start A TypeScript Project With NodeJS",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --watch src/**/*.ts --exec \"ts-node\" src/app.ts",
    "demo": "tsc -w src/demo/index.ts",
    "build": "cross-env NODE_ENV=production rollup -c --bundleConfigAsCjs",
    "dev": "cross-env NODE_ENV=development rollup -c --bundleConfigAsCjs -w"
  },
  "keywords": [],
  "author": "baizhi958216",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.15.11",
    "axios": "^1.3.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.13",
    "rollup": "^3.20.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0"
  }
}
